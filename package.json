{
    "name": "generator-mkstarter-android",
    "description": "Starts android projects with tshion's templates.",
    "engines": {
        "npm": ">= 4.0.0"
    },
    "eslintConfig": {
        "extends": [
            "xo",
            "prettier"
        ],
        "env": {
            "jest": true,
            "node": true
        },
        "rules": {
            "prettier/prettier": "error"
        },
        "plugins": [
            "prettier"
        ]
    },
    "files": [
        "generators"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "jest": {
        "testEnvironment": "node"
    },
    "license": "MIT",
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "main": "generators/index.js",
    "keywords": [
        "android",
        "yeoman-generator"
    ],
    "repository": "tshion/generator-mkstarter-android",
    "version": "0.0.1",
    "scripts": {
        "pretest": "eslint .",
        "test": "jest"
    },
    "dependencies": {
        "chalk": "^2.1.0",
        "yeoman-generator": "^5.6.1",
        "yosay": "^2.0.2"
    },
    "devDependencies": {
        "coveralls": "^3.0.7",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.6.0",
        "eslint-config-xo": "^0.27.2",
        "eslint-plugin-prettier": "^3.1.1",
        "husky": "^3.0.9",
        "jest": "^26.1.0",
        "lint-staged": "^12.3.7",
        "prettier": "^1.19.1",
        "yeoman-assert": "^3.1.1",
        "yeoman-test": "^1.7.0"
    }
}