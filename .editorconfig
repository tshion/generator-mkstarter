# http://editorconfig.org
root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.{js,jsx,ts,tsx}]
indent_size = 2
quote_type = single

[*.json]
insert_final_newline = false

[*.md]
max_line_length = off
insert_final_newline = false
trim_trailing_whitespace = false
