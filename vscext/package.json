    "devDependencies": {
        "@types/node": "14.x",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "eslint": "^8.11.0",
        "ts-loader": "^9.2.8",
        "typescript": "^4.5.5",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2"
